                        <visibleWhen>
                            <reference definitionId="com.vectrace.MercurialEclipse.def.mercurial"/>
                        </visibleWhen>
                    </command>
                </menu>      
				    <command
                        commandId="com.vectrace.MercurialEclipse.menu.SignHandler"
