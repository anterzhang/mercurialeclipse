                          label="%menuLabel.strip">
                        <visibleWhen>
                            <reference definitionId="com.vectrace.MercurialEclipse.def.mercurial"/>
                        </visibleWhen>
                    </command>
                </menu>				
				<separator name="group6" visible="true"/>				                               
